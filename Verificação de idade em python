x = float(input("qual o ano do seu nascimento?"))
z = float(input("estamos em qual ano?"))
y = z - x
if y >=16 and y<18:
    print("ja tem idade para votar mas não para tirar a CNH")
    print("sua idade é:",y,"anos")
    print("voce nasceu no ano:",x)
elif y>=18:
    print("ja tem idade para tirar a CNH e para votar")
    print("sua idade é:", y, "anos")
    print("voce nasceu no ano:", x)
else:
    print("não tem idade para votar")
    print("sua idade é:",y,"anos")
    print("voce nasceu no ano:",x)
